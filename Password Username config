import re
a = input("Give the Password now lol:")
s = input("Give me DA NAME:")



yeet1 = re.compile("[A-Z]+")
yeet2 = re.compile("[0-9]+")
yeet3 = re.compile("[a-z]+")
name = re.compile("[\w]+@[\w]+\.+[\w]")



if yeet1.findall(a) and yeet2.findall(a) and yeet3.findall(a):
    if name.findall(s):
        inputAlpha = (input())
        inputBeta = (input())
        print("WOW. Took you long enough")
    else:
        print("Lmao you got it wrong again")


